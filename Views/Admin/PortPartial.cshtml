@using Galleass.Models
@model Port
<form asp-action="NewPort" asp-controller="Admin" method="Post">
    <div>
        <span asp-validation-for="PortName"></span><br>
        <label asp-for="PortName"></label><br>
        <input asp-for="PortName">
    </div>
    <div>
        <span asp-validation-for="PortImageURL"></span><br>
        <label asp-for="PortImageURL"></label><br>
        <input asp-for="PortImageURL">
    </div>
    <div>
        <span asp-validation-for="Description"></span><br>
        <label asp-for="Description"></label><br>
        <input asp-for="Description">
    </div>
    <div>
        <input class="submit" type="submit" value="Create Port">
    </div>
</form>