@{
    ViewData["Title"] = "Home Page";
}
<header id="adminHeader">
    <h1>GALLEASS ADMIN: @ViewBag.Admin</h1>
</header>
<aside id="adminAside">
    <div class="aside" id="mapMake">
        <h3>Create Map Size:</h3>
        <partial name="MapPartial"></partial>
    </div>
    <div class="aside" id="portMaker">
        <h3>Create a Port:</h3>
        <partial name="PortPartial"></partial>
    </div>
    <div class="aside" id="goodMaker">
        <h3>Create a TradeGood:</h3>
        <partial name="GoodsPartial"></partial>
    </div>
    <div class="aside" id="shipMaker">
        <h3>Create a Ship:</h3>
        <partial name="ShipPartial"></partial>
    </div>

</aside>
<main id="adminMain">
    <canvas>
    Your browser does not support HTML5 Canvas.
    </canvas>
</main>
<aside id="db">
    <div>
        <h3>Ports:</h3>
        @foreach(var p in @ViewBag.Ports)
        {
            <p>@p.PortName</p>
        }
    </div>
    <div>
        <h3>Trade Goods:</h3>
        @foreach(var g in @ViewBag.TradeGoods)
        {
            <p>@g.GoodName</p>
        }
    </div>
    <div>
        <h3>Ships:</h3>
        @foreach(var s in @ViewBag.Ships)
        {
            <p>@s.TypeName</p>
        }
    </div>
</aside>